<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE hibernate-configuration PUBLIC
          "-//Hibernate/Hibernate Configuration DTD 3.0//EN"
          "http://hibernate.sourceforge.net/hibernate-configuration-3.0.dtd">
<!-- Generated by MyEclipse Hibernate Tools. -->
<hibernate-configuration>

<session-factory>
	<property name="dialect">
		org.hibernate.dialect.MySQLDialect
	</property>
	<property name="autoReconnect">true</property>
	<property name="show_sql">false</property>
	<property name="show_format">true</property>
	<property name="myeclipse.connection.profile">shsmis</property>
	<property name="hibernate.proxool.pool_alias">proxoolPool</property>
	<property name="hibernate.proxool.xml">ProxoolConf.xml</property>
	<property name="connection.provider_class">
		org.hibernate.connection.ProxoolConnectionProvider
	</property>

	<property name="myeclipse.connection.profile">mymvc</property>
	<mapping resource="org/model/User.hbm.xml" />
	<mapping resource="org/model/Log.hbm.xml" />
	<mapping resource="org/model/UserDetail.hbm.xml" />
	<mapping resource="org/model/Fileio.hbm.xml" />
	
	<mapping resource="org/view/VLog.hbm.xml" />
	<mapping resource="org/view/VUser.hbm.xml" />
	<mapping resource="org/view/VFile.hbm.xml" />

</session-factory>

</hibernate-configuration>